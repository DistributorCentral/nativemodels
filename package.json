{
	"author": {
		"email": "prefinem@gmail.com",
		"name": "Prefinem",
		"url": "https://prefinem.com"
	},
	"description": "Native Models for Javascript",
	"devDependencies": {
		"@babel/cli": "^7.1.0",
		"@babel/core": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"babel-eslint": "^10.0.0",
		"eslint": "^5.6.0",
		"eslint-config-prettier": "^3.0.1",
		"husky": "^1.0.0",
		"jest": "^23.6.0",
		"lint-staged": "^7.3.0",
		"prettier": "^1.14.3"
	},
	"homepage": "https://github.com/Prefinem/nativemodels",
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"main": "index.js",
	"name": "nativemodels",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prefinem/nativemodels"
	},
	"scripts": {
		"build": "./node_modules/.bin/babel src/ --out-dir dist",
		"clean": "yarn lint && yarn prettify",
		"docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
		"docs:md": "jsdoc2md src/** >> DOCS.md",
		"lint": "eslint src/* --fix",
		"precommit": "yarn staged",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"pub": "./publish.sh",
		"staged": "lint-staged",
		"test": "jest --coverage",
		"test-dist": "yarn build && cp -R tests/ tests-dist/ && jest"
	},
	"version": "1.1.0"
}
