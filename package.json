{
	"author": {
		"email": "prefinem@gmail.com",
		"name": "Prefinem",
		"url": "https://prefinem.com"
	},
	"description": "Native Models for Javascript",
	"devDependencies": {
		"eslint": "^5.6.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-plugin-jest": "^22.0.0",
		"husky": "^1.2.0",
		"jest": "^23.6.0",
		"lint-staged": "^8.0.0",
		"prettier": "^1.14.3"
	},
	"engines": {
		"node": ">=8.10"
	},
	"homepage": "https://github.com/Prefinem/nativemodels",
	"husky": {
		"hooks": {
			"pre-commit": "yarn staged"
		}
	},
	"license": "MIT",
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"prettier --config ./.prettierrc.json --write",
			"jest --bail --findRelatedTests",
			"git add"
		]
	},
	"main": "index.js",
	"name": "nativemodels",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Prefinem/nativemodels"
	},
	"scripts": {
		"clean": "yarn lint && yarn prettify",
		"eslint-check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint": "eslint src/* --fix",
		"prettify": "prettier --config ./.prettierrc.json --write \"src/**\"",
		"staged": "lint-staged",
		"test": "jest --coverage"
	},
	"version": "2.0.0"
}
